<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.stone.govjob"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="22" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.stone.govjob.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.stone.govjob.QueryJob"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.stone.govjob.About"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.stone.govjob.Favorite"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.stone.govjob.JobDetail"
            android:label="@string/app_name" >
        </activity>
    </application>

</manifest>